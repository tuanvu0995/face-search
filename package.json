{
  "name": "face-search",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.2.3",
    "@types/lodash": "^4.14.195",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.6",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.4",
    "typescript": "~4.6",
    "youch": "^3.2.3",
    "youch-terminal": "^2.2.0"
  },
  "dependencies": {
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/lucid": "^18.4.0",
    "@tensorflow/tfjs-node": "^4.2.0",
    "@tensorflow/tfjs-node-gpu": "^4.2.0",
    "@vladmandic/face-api": "^1.7.9",
    "@vladmandic/human": "^3.0.6",
    "axios": "^1.3.2",
    "canvas": "^2.11.0",
    "classic-level": "^1.2.0",
    "image-size": "^1.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "mysql2": "^3.3.3",
    "nanoid": "^3.3.4",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.32.1",
    "source-map-support": "^0.5.21",
    "uniqid": "^5.4.0"
  }
}
